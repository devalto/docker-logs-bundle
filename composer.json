{
  "name": "chrif/docker-logs-bundle",
  "type": "symfony-bundle",
  "description": "DockerLogsBundle is intended for containerized Symfony applications.",
  "keywords": [
    "log",
    "logging",
    "docker"
  ],
  "homepage": "https://github.com/chrif",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Fecteau"
    }
  ],
  "require": {
    "php": ">=7.0",
    "symfony/monolog-bundle": "~3.3",
    "symfony/dependency-injection": "~4.0",
    "symfony/config": "~4.0",
    "symfony/console": "~4.0",
    "symfony/yaml": "~4.0"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "Chrif\\Bundle\\DockerLogsBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  }
}
